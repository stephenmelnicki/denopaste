{
  "nodeModulesDir": "auto",
  "tasks": {
    "ok": "deno fmt --check && deno lint && deno task check:types && deno task test",
    "check:types": "deno check --allow-import",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "start": "deno run -A main.ts",
    "test": "deno test -A --doc --ignore=e2e/",
    "e2e": "deno test -A e2e/",
    "update": "deno run -Ar jsr:@fresh/update"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": ["coverage/", "**/_fresh/*", "node_modules"],
  "imports": {
    "@astral/astral": "jsr:@astral/astral@^0.4.7",
    "@deno/kv-utils": "jsr:@deno/kv-utils@^0.1.3",
    "@preact-icons/go": "jsr:@preact-icons/go@^1.0.12",
    "@std/expect": "jsr:@std/expect@^1.0.3",
    "@std/testing": "jsr:@std/testing@^1.0.3",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "fresh": "jsr:@fresh/core@^2.0.0-alpha.29",
    "@fresh/plugin-tailwind": "jsr:@fresh/plugin-tailwind@^0.0.1-alpha.7",
    "preact": "npm:preact@^10.25.4",
    "@preact/signals": "npm:@preact/signals@^2.0.1",
    "tailwindcss": "npm:tailwindcss@3.4.3",
    "components/": "./components/",
    "data/": "./data/",
    "islands/": "./islands/",
    "middlewares/": "./middlewares/",
    "utils/": "./utils/"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  },
  "unstable": [
    "kv"
  ]
}
